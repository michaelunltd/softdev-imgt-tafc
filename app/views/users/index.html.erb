<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>
<% @users.each do |user| %>
  <%= "#{user.last_name}, #{user.first_name}" %>
  <%= link_to 'Edit', edit_user_path(user) %>
  <%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %>
  <p>
    Username: <%= user.username %>
  </p>
  <p>
    Role: <%= user.role %>
  </p>
<% end %>
