<!DOCTYPE html>
<html>

<head>
  <title>SoftdevIMgtTafc</title>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= custom_bootstrap_flash %>
      <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
        <%= csrf_meta_tags %>
</head>

<body>
  <%= render "shared/navbar" %>
    <div class="container-fluid">
      <div class="row row-offcanvas row-offcanvas-left">
        <%= render "shared/sidebar" %>
          <div class="col-sm-9 col-md-10 main">
            <p class="visible-xs">
              <button class="btn btn-primary btn-xs" data-toggle="offcanvas" type="button">
                <i class="glyphicon glyphicon-chevron-left"></i>
              </button>
            </p>
              <div class="row" id="main-content">
                <%= yield %>
              </div>
          </div>
      </div>
      <%= render "shared/footer" %>
</body>

<%= javascript_include_tag 'datatables', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'datatables-bootstrap', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'material-bonus', 'data-turbolinks-track' => true %>

</html>
