<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>
<%= form_tag check_login_path do %>
  <ul class="formList">
    <li>
      <%= label_tag :username %>
      <%= text_field_tag :username %>
    </li>
    <li>
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    </li>
    <li>
      <%= submit_tag "Log In" %>
    </li>
  </ul>
<% end %>
